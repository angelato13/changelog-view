{
  "name": "changelog-view",
  "version": "1.0.1",
  "description": "Tool to view changelog of package in console",
  "bin": {
    "changelog-view": "bin/changelog-view"
  },
  "repository": "https://github.com/jdeniau/changelog-view",
  "author": "Julien Deniau <julien.deniau@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "find-versions": "^2.0.0",
    "github": "^9.2.0",
    "jest": "^20.0.4",
    "marked": "^0.3.6",
    "marked-terminal": "^2.0.0",
    "marked-to-md": "^1.0.1",
    "minimist": "^1.2.0",
    "node-fetch": "^1.7.3",
    "semver": "^5.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "lint-staged": "^4.2.0",
    "nock": "^9.0.14",
    "prettier": "^1.6.1",
    "rollup": "^0.49.3",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "scripts": {
    "test": "jest",
    "build": "rollup -c && chmod a+x bin/changelog-view",
    "prepublish": "npm run build",
    "precommit": "lint-staged"
  },
  "jest": {
    "transform": {
      ".+\\.(js)$": "babel-jest",
      ".+\\.(md)$": "<rootDir>/fileTransformer.js"
    }
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
