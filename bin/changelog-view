var argv$1 = process.argv;

var supportsColors = (function () {
  if (argv$1.indexOf('--no-color') !== -1 ||
    argv$1.indexOf('--color=false') !== -1) {
    return false;
  }

  if (argv$1.indexOf('--color') !== -1 ||
    argv$1.indexOf('--color=true') !== -1 ||
    argv$1.indexOf('--color=always') !== -1) {
    return true;
  }

  if (process.stdout && !process.stdout.isTTY) {
    return false;
  }

  if (process.platform === 'win32') {
    return true;
  }

  if ('COLORTERM' in process.env) {
    return true;
  }

  if (process.env.TERM === 'dumb') {
    return false;
  }

  if (/^screen|^xterm|^vt100|color|ansi|cygwin|linux/i.test(process.env.TERM)) {
    return true;
  }

  return false;
})();

